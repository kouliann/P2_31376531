<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="/stylesheets/querys.css">
        <!-- Etiquetas Open Graph -->
    <meta property="og:title" content="<%= og && og.title ? og.title : title %>">
    <meta property="og:description" content="<%= og && og.description ? og.description : '' %>">
    <meta property="og:type" content="website">
  </head>
  <body>
    <header>
        <nav>
            <img src="/images/logo.jpg" alt="logo">
            <ul>  
                <li><a href="/">Inicio</a></li>
                <li><a href="#ventajas">Nosotros</a></li>
                <li><a href="#servicios">Servicios</a></li>
                <li><a href="#contacto">Contacto</a></li>
            </ul>
        </nav>
    </header>
    <body>
        <nav class="mainNav">
      <h2>Administradores</h2>
            
    <% if (users && users.length > 0) { %>
        <table>
            <thead>
                <tr class="tableHeader">
                    <th>ID</th>
                    <th>Email</th>
                    <th>Fecha de registro</th>
                </tr>
            </thead>
            
            <tbody>
                <% users.forEach(function(user) { %>
                    <tr>
                        <td class="tableTd"><%= user.id %></td>
                        <td class="tableTd"><%= user.email%></td>
                        <td class="tableTd"><%= user.created_at %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    <% } else { %>
        <p>No hay usuarios registrados en la base de datos.</p>
    <% } %>
    </nav>
    <footer>
        <img src="/images/logo.jpg" alt="logo">
        <p>&copy; 2023 Safe&Home. Todos los derechos reservados.</p>
    </footer>
    </body>
